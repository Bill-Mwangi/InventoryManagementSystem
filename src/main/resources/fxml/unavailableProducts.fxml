<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.bill.controllers.UnavailableProductsController"
      prefHeight="400.0" prefWidth="600.0">
    <padding><Insets bottom="25" left="25" right="25" top="25" /> </padding>

    <TableView fx:id="productsTable">
        <columns>
            <TableColumn text="Product ID" prefWidth="100" fx:id="productID"/>
            <TableColumn text="Product Name" prefWidth="100" fx:id="productName"/>
            <TableColumn text="Description" prefWidth="100" fx:id="description"/>
            <TableColumn text="Quantity" prefWidth="100" fx:id="quantity"/>
        </columns>

    </TableView>

    <VBox alignment="BOTTOM_CENTER">
        <Button onAction="#handleBackButton" text="Back" prefHeight="40" prefWidth="80"/>
    </VBox>

</VBox>